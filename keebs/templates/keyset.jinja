{% extends "layout.jinja" %}
{% block sidebar %}
    <p class='text-muted'>A list of fancy keysets</p>
    <li class="list-group-item list-group-item-light">GMK</li>
    <li class="list-group-item list-group-item-light">SA</li>
    <li class="list-group-item list-group-item-light">Cherry</li>
{% endblock sidebar %}
{% block content %}
    <div class="content-section">
        <a class="mr-2" href="#">{{ keyset.brand }}</a><small>{{ keyset.name }}</small>
        <hr class="divider"/>
        <h2><a class="article-title" href="#">{{ keyset.name }}</a></h2>
        <p>{{ keyset.name }}</p>
    </div>
{% endblock content %}